{
  "name": "koa-api-boilerplate",
  "version": "0.0.1",
  "repository": "koajs/api-boilerplate",
  "description": "Koa API boilerplate application",
  "scripts": {
    "start": "node bin/api",
    "test": "RETHINKDB_DB=test NODE_TLS_REJECT_UNAUTHORIZED=0 mocha --require co-mocha --require should --reporter spec --timeout 20000 --growl  ./tests/*spec.js"
