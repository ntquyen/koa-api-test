    "koa": "~0.6.1",
    "koa-compress": "1.0.7",
    "koa-logger": "~1.2.1",
    "koa-ratelimit": "~1.0.3",
    "koa-resource-router": "^0.3.3",
    "koa-response-time": "~1.0.2",
    "koa-router": "~3.1.4",
    "mocha": "^2.3.2",
    "redis": "~0.10.3",
    "rethinkdbdash": "^2.1.7",
    "should": "^7.1.0"
  },
  "devDependencies": {
    "co-mocha": "^1.1.2",
    "mocha": "*",
    "should": "*",
    "supertest": "~0.8.2"
